{
	"name": "tachyon-drive-node-fs",
	"version": "0.10.1",
	"description": "Nodejs fs module storage driver implementation",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		}
	},
	"scripts": {
		"build": "tsup src/index.ts --minify --sourcemap --format cjs,esm --dts --clean",
		"prepublishOnly": "npm run build",
		"test": "vitest test --run --no-isolate --coverage",
		"coverage": "vitest test --run --no-isolate --reporter=dot --coverage --coverage.reporter=lcov",
		"lint": "eslint . --ext .ts",
		"validate": "tsc --noEmit --project tsconfig.test.json"
	},
	"files": [
		"dist"
	],
	"repository": "github:mharj/tachyon-drive-node-fs",
	"keywords": [
		"storage",
		"driver",
		"nodejs"
	],
	"author": "mharj",
	"license": "MIT",
	"devDependencies": {
		"@avanio/logger-like": "^0.2.11",
		"@stylistic/eslint-plugin": "^3.1.0",
		"@stylistic/eslint-plugin-ts": "^3.1.0",
		"@tsconfig/node20": "^20.1.4",
		"@types/node": "^22.13.5",
		"@types/sinon": "^17.0.4",
		"@typescript-eslint/eslint-plugin": "^8.24.1",
		"@typescript-eslint/parser": "^8.24.1",
		"@vitest/coverage-v8": "^3.0.6",
		"c8": "^10.1.3",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^10.0.1",
		"eslint-config-standard": "^17.1.0",
		"eslint-import-resolver-typescript": "^3.8.3",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-prettier": "^5.2.3",
		"eslint-plugin-sonarjs": "^0.23.0",
		"prettier": "^3.5.2",
		"sinon": "^19.0.2",
		"tachyon-drive": "^0.11.1",
		"ts-node": "^10.9.2",
		"tslib": "^2.8.1",
		"tsup": "^8.3.6",
		"typescript": "^5.7.3",
		"vite": "^6.1.1",
		"vitest": "^3.0.6",
		"zod": "^3.24.2"
	},
	"dependencies": {
		"@luolapeikko/ts-common": "^0.2.8"
	},
