{
	"name": "tachyon-drive-node-fs",
	"version": "0.10.0",
	"description": "Nodejs fs module storage driver implementation",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		}
	},
	"scripts": {
		"build": "tsup src/index.ts --minify --sourcemap --format cjs,esm --dts --clean",
		"prepublishOnly": "npm run build",
		"test": "vitest test --run --no-isolate --coverage",
		"coverage": "vitest test --run --no-isolate --reporter=dot --coverage --coverage.reporter=lcov",
		"lint": "eslint . --ext .ts",
		"validate": "tsc --noEmit --project tsconfig.test.json"
	},
	"files": [
		"dist"
	],
	"repository": "github:mharj/tachyon-drive-node-fs",
	"keywords": [
		"storage",
		"driver",
		"nodejs"
	],
	"author": "mharj",
	"license": "MIT",
	"devDependencies": {
		"@avanio/logger-like": "^0.2.7",
		"@stylistic/eslint-plugin": "^2.10.1",
		"@stylistic/eslint-plugin-ts": "^2.10.1",
		"@tsconfig/node18": "^18.2.4",
		"@types/node": "^18.19.64",
		"@types/sinon": "^17.0.3",
		"@typescript-eslint/eslint-plugin": "^8.14.0",
		"@typescript-eslint/parser": "^8.14.0",
		"@vitest/coverage-v8": "^2.1.5",
		"@vitest/ui": "^2.1.5",
		"c8": "^10.1.2",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-standard": "^17.1.0",
		"eslint-plugin-deprecation": "^3.0.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-sonarjs": "^0.23.0",
		"prettier": "^3.3.3",
		"sinon": "^19.0.2",
		"tachyon-drive": "^0.10.2",
		"ts-node": "^10.9.2",
		"tslib": "^2.8.1",
		"tsup": "^8.3.5",
		"typescript": "^5.6.3",
		"vite": "^5.4.11",
		"vitest": "^2.1.5",
		"zod": "^3.23.8"
	},
	"dependencies": {
		"@luolapeikko/ts-common": "^0.2.4"
	},
	"peerDependencies": {
		"@avanio/logger-like": "^0.1 || ^0.2",
		"tachyon-drive": "^0.9 || ^0.10"
	}
}
